import java.util.*;

class Record{
	int Roll_no;
	String Name;
	Record (){
//		this(32358, "Om");
		System.out.println("This is from Default Constructor.");
	}
	Record(int roll_no, String name){
		this();
		this.Roll_no = roll_no;
		this.Name = name;
		System.out.println("Hello " + Roll_no + "_" + Name + ". This from parametrised constructor.");
	}
}

public class ConstructorOverloading {

	public static void main(String[] args) {
		System.out.println("Name: Aman Mugut");
		System.out.println("Roll no: 32338");
		System.out.println("Batch: L7");
		// TODO Auto-generated method stub
		Record obj = new Record(32338, "Aman");
		
	}

}
